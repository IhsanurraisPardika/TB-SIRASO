generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int    @id @default(autoincrement())
  fullname String
  username String @unique
  phone    String
  password String
  status   String
  alamat   String
  email    String 
}
